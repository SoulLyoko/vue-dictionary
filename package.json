{
  "name": "vue-dictionary",
  "version": "1.0.4",
  "description": "vue-dictionary for admin-system",
  "keywords": [
    "vue",
    "dict",
    "dictionay"
  ],
  "author": "Lyoko <519052532@qq.com>",
  "license": "MIT",
  "private": false,
  "main": "lib/vue-dictionary.umd.min.js",
